  <script defer src="{{asset "js/app.js"}}"></script>

  <!-- TocBot JS -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.10.0/tocbot.min.js"></script>
  <script>
      tocbot.init({
          tocSelector: '.toc',
          contentSelector: '.m-article-hero__content',
          hasInnerContainers: true
      });
  </script>
  <!-- end of TocBot -->

